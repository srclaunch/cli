{
  "name": "@srclaunch/cli",
  "description": "Command line tools used by SrcLaunch applications and services",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.540",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "srclaunch": "dist/index.mjs"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build:cjs": "esbuild --external:'@rauschma/stringio' --external:'@yarnpkg/core' --external:c8 --external:esbuild --external:'fs-extra' --external:ink  --external:'jest-cli' --external:'js-yaml' --external:'latest-version' --external:meow --external:ora --external:prompts --external:react --external:'simple-git' --external:'standard-version' --external:typescript --external:unconfig --external:vite --external:'@vitejs/plugin-react' --external:'update-notifier' --sourcemap --bundle --minify --outfile=dist/index.cjs --format=cjs --target=esnext --define:'navigator={}' --platform=node src/index.ts",
    "build:es": "esbuild --external:'@rauschma/stringio' --external:'@yarnpkg/core' --external:c8 --external:esbuild --external:'fs-extra' --external:ink --external:'jest-cli' --external:'js-yaml' --external:'latest-version' --external:meow --external:ora --external:prompts --external:react --external:'simple-git' --external:'standard-version' --external:typescript --external:unconfig --external:vite --external:'@vitejs/plugin-react' --external:'update-notifier' --sourcemap --bundle --minify  --outfile=dist/index.mjs --format=esm --target=esnext --define:'navigator={}' --platform=node src/index.ts",
    "build:self": "node ./dist/index.js build",
    "build": "yarn clean && yarn build:cjs && yarn build:es",
    "clean": "rm -fr dist",
    "dev": "NODE_ENV=dev && node --loader ts-node/esm --experimental-specifier-resolution=node ./src/index.ts",
    "git:push": "git push --follow-tags origin main",
    "qr": "yarn build && git add . && git commit -m \"Quick release\" && yarn release",
    "release": "yarn version:patch && yarn git:push",
    "start": "node --experimental-modules --experimental-specifier-resolution=node dist/index.js",
    "test": "yarn ava ./src/__tests__/**",
    "test:coverage": "yarn c8 --all --clean --include src --exclude 'src/__tests__/**' ava ./src/__tests__/** && yarn c8 report --all --reporter json-summary --reporter lcov --include src --exclude 'src/__tests__/**'",
    "test:watch": "yarn ava --watch",
    "version:patch": "standard-version",
    "yui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@srclaunch/types": "^0.1.185",
    "@types/diff": "^5.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.5",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.32",
    "@types/node-emoji": "^1.8.1",
    "@types/pluralize": "^0.0.29",
    "@types/prompts": "^2.4.0",
    "@types/react": "^18.0.9",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.9",
    "@types/standard-version": "^7.0.1",
    "@types/update-notifier": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-stylelint": "^15.1.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-regexp": "^1.7.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.4.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0"
  },
  "dependencies": {
    "@rauschma/stringio": "^1.4.0",
    "@srclaunch/dx": "^0.1.71",
    "@srclaunch/exceptions": "^0.1.72",
    "@srclaunch/node-environment": "^0.1.23",
    "@vitejs/plugin-react": "^1.3.2",
    "@yarnpkg/core": "^4.0.0-rc.5",
    "ava": "^4.2.0",
    "c8": "^7.11.2",
    "chalk": "^5.0.1",
    "change-case": "^4.1.2",
    "css-tree": "^2.1.0",
    "diff": "^5.0.0",
    "dir-glob": "^3.0.1",
    "esbuild": "^0.14.38",
    "fs-extra": "^10.1.0",
    "ink": "^3.2.0",
    "ink-multi-select": "^2.0.0",
    "ink-spinner": "^4.0.3",
    "jest": "^27",
    "jest-cli": "^27",
    "js-yaml": "^4.1.0",
    "latest-version": "^6.0.0",
    "luxon": "^2.4.0",
    "meow": "^10.1.2",
    "nanoid": "^3.3.4",
    "ora": "^6.1.0",
    "pluralize": "^8.0.0",
    "prompts": "^2.4.2",
    "react": "^18.1.0",
    "semver": "^7.3.7",
    "shelljs": "^0.8.5",
    "simple-git": "^3.7.1",
    "standard-version": "^9.3.2",
    "terminal-link": "^3.0.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "unconfig": "^0.3.4",
    "update-notifier": "^5.1.0",
    "vite": "^2.9.8"
  },
  "packageManager": "yarn@3.2.0"
}
