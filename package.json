{
  "name": "@srclaunch/cli",
  "description": "Command line tools used by SrcLaunch applications and services",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "ISC",
  "version": "0.1.103",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "srclaunch": "dist/index.js"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build:es": "esbuild --sourcemap --bundle --minify --outfile=dist/index.js --external:'meow' --external:'fs-extra' --external:'esbuild-css-modules-plugin' --external:'typescript' --external:'esbuild' --external:'update-notifier'  --external:vite --format=esm --target=esnext --define:'navigator={}' --platform=node src/index.ts",
    "build:types": "tsc --build --force",
    "build": "yarn clean && yarn build:es && yarn build:types",
    "clean": "rm -fr ./dist && mkdir dist",
    "dev": "NODE_ENV=dev && node --loader ts-node/esm --experimental-specifier-resolution=node ./src/index.ts",
    "git:push": "git push --follow-tags origin main",
    "qr": "yarn build && git add . && git commit -m \"Quick release - patch\" && yarn release",
    "release": "yarn version:patch && yarn git:push",
    "start": "node dist/index.js",
    "test": "yarn ava",
    "test:coverage": "yarn c8 ava --all --clean --include src && yarn c8 report --all  --reporter json-summary --include src ",
    "test:watch": "yarn ava --watch",
    "version:patch": "standard-version",
    "yui": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@srclaunch/exceptions": "^0.1.42",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "add": "^2.0.6",
    "chalk": "^5.0.1",
    "change-case": "^4.1.2",
    "css-tree": "^2.1.0",
    "esbuild": "^0.14.36",
    "fs-extra": "^10.0.1",
    "ink": "^3.2.0",
    "ink-spinner": "^4.0.3",
    "luxon": "^2.3.1",
    "meow": "^10.1.2",
    "nanoid": "^3.3.2",
    "pluralize": "^8.0.0",
    "react": "^18.0.0",
    "shelljs": "^0.8.5",
    "terminal-link": "^3.0.0",
    "typescript": "^4.6.3",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@srclaunch/dx": "^0.1.19",
    "@srclaunch/types": "^0.1.63",
    "@types/fs-extra": "^9.0.13",
    "@types/luxon": "^2.3.1",
    "@types/node": "17.0.24",
    "@types/node-emoji": "^1.8.1",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^18.0.5",
    "@types/rimraf": "^3.0.2",
    "@types/update-notifier": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-react": "^1.3.1",
    "ava": "^4.2.0",
    "c8": "^7.11.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-regexp": "^1.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.3.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.7.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.7.0",
    "vite": "^2.9.5"
  },
  "resolve": {
    "alias": {
      "react/jsx-dev-runtime": "react/jsx-dev-runtime.js",
      "react/jsx-runtime": "react/jsx-runtime.js"
    }
  }
}
